<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.euserprovider.dao.UserDao">

    <sql id="display">
        
    </sql>

    <resultMap id="UserMap" type="com.example.emodeldao.user.po.User">
        <id column="id" property="id"/>
        <result column="userName" property="user_name"/>
        <result column="userNickName" property="user_nickname"/>
        <result column="userPwd" property="user_pwd"/>
        <result column="userQuestionEncrypted" property="user_question_encrypted"/>
        <result column="userQuestionEncryptedAnswer" property="user_question_encrypted_answer"/>
        <result column="tel" property="tel"/>
        <result column="email" property="email"/>
        <result column="IDCard" property="ID_card"/>
        <result column="realName" property="real_name"/>
        <!--这个字段先不配置-->
        <result column="" property="t_credit_value_id"/>
        <result column="creationTime" property="creation_time"/>
        <result column="modifyTime" property="modify_time"/>
        <result column="modifyId" property="modify_id"/>
        <result column="type" property="type"/>
    </resultMap>

    <select id="findById" parameterType="int" resultMap="UserMap">
        select <include refid="display"/> from `t_user` where `id` = #{id}
    </select>


</mapper>